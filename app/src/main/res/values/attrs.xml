<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="circleview">
        <attr name="radius" format="integer" />
        <attr name="fillColor" format="color" />
        <attr name="strokeWidth" format="color" />
        <attr name="strokeColor" format="color" />
        <attr name="angleStart" format="integer" />
        <attr name="angleEnd" format="integer" />
    </declare-styleable>

    <declare-styleable name="badge">
        <attr name="badge_text" format="string" />
        <attr name="badge_textSize" format="integer" />
        <attr name="badge_fillColor" format="color" />
        <attr name="badge_textColor" format="color" />
    </declare-styleable>

</resources>